{
  "name": "tamagui-expo",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "upgrade:tamagui": "yarn up '*tamagui*'@latest '@tamagui/*'@latest",
    "upgrade:tamagui:canary": "yarn up '*tamagui*'@canary '@tamagui/*'@canary",
    "start": "expo start",
    "android": "expo run:android",
    "android:release": "expo run:android --variant release",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "prebuild": "expo prebuild -p android",
    "build": "npx react-native build-android --tasks assembleRelease",
    "codegen": "npx typed-pocketbase -o __generated__/schema.ts",
    "lint": "eslint .",
    "editor:dev": "vite --config ./tentap/editor-web/vite.config.ts",
    "editor:build": "vite --config ./tentap/editor-web/vite.config.ts build && yarn editor:post-build",
    "editor:post-build": "node ./node_modules/@10play/tentap-editor/scripts/buildEditor.js ./tentap/editor-web/build/index.html",
    "reverse-android": "adb reverse tcp:3000 tcp:3000"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@10play/tentap-editor": "^0.4.58",
    "@expo/metro-runtime": "~3.1.3",
    "@fakerepic/typed-pocketbase": "fakerepic/typed-pocketbase#remove_pnpm",
    "@gorhom/bottom-sheet": "^4",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-navigation/drawer": "^6.6.15",
    "@react-navigation/native": "^6.0.2",
    "@shopify/flash-list": "1.6.3",
    "@tamagui/config": "^1.92.1",
    "@tamagui/metro-plugin": "^1.92.1",
    "@tanstack/react-query": "^5.28.6",
    "@types/pouchdb": "^6.4.2",
    "babel-preset-expo": "^10.0.1",
    "buffer": "^6.0.3",
    "events": "^3.3.0",
    "expo": "~50.0.6",
    "expo-av": "~13.10.6",
    "expo-build-properties": "~0.11.1",
    "expo-crypto": "~12.8.1",
    "expo-font": "~11.10.2",
    "expo-image-picker": "~14.7.1",
    "expo-linear-gradient": "~12.7.2",
    "expo-linking": "~6.2.1",
    "expo-router": "~3.4.7",
    "expo-splash-screen": "~0.26.4",
    "expo-sqlite": "~13.4.0",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "~2.9.3",
    "expo-web-browser": "~12.8.2",
    "lodash": "4.17.21",
    "pocketbase": "^0.21.1",
    "pouchdb-adapter-http": "^8.0.1",
    "pouchdb-adapter-utils": "^8.0.1",
    "pouchdb-adapter-websql-core": "^7.0.0",
    "pouchdb-browser": "^8.0.1",
    "pouchdb-core": "^8.0.1",
    "pouchdb-find": "^8.0.1",
    "pouchdb-replication": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "~2.0.4",
    "react-hook-form": "^7.51.1",
    "react-native": "0.73.4",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-get-random-values": "~1.8.0",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-web": "^0.19.9",
    "react-native-webview": "13.6.4",
    "tamagui": "^1.92.1",
    "typescript": "^5.3.3",
    "use-pouchdb": "^2.0.2",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@types/eslint": "^8",
    "@types/events": "^3",
    "@types/lodash": "^4",
    "@types/react": "~18.2.14",
    "@types/react-native-get-random-values": "^1",
    "@vitejs/plugin-react": "^4.3.0",
    "eslint": "8",
    "eslint-config-expo": "^7.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "vite": "^5.2.11",
    "vite-plugin-singlefile": "^2.0.1"
  },
  "private": true,
  "packageManager": "yarn@4.0.2"
}
